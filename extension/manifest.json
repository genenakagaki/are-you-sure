{
  "manifest_version": 2,
  "name": "are-you-sure",
  "description": "testing",
  "version": "0.0.1",
  "icons": {
    "64": "icons/icon.png"
  },
  "background": {
    "scripts": [
      "background_script.js"
    ]
  },
  "browser_action": {
    "default_icon": {
      "64": "icons/icon.png"
    },
    "default_popup": "browserAction/index.html",
    "default_title": "are-you-sure"
  },
  "options_ui": {
    "page": "options/index.html"
  },
  "permissions": ["scripting", "webRequest", "webRequestBlocking", "<all_urls>"]
}
